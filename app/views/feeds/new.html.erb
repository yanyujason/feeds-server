<h1>New Entry</h1>
<%= form_for :feeds, url: feeds_path do |f| %>

    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :link %><br>
      <%= f.text_field :link %>
    </p>

    <p>
      <%= f.label :author %><br>
      <%= f.text_field :author %>
    </p>

    <p>
      <%= f.label :source %><br>
      <%= f.text_field :source %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>